{
  "name": "qbc-server",
  "version": "1.0.0",
  "repository": "git@github.com:seanye24/qbc-server.git",
  "author": "Sean Ye <seanye24@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "start": "npm run start:server",
    "start:server": "node -r dotenv/config dist/index.js",
    "build:full": "npm run build:ui && npm run build:server",
    "build:ui": "mkdir -p dist && cd ../qbhub-frontend && yarn build && cp -r build ../qbhub-server/dist && cd ../qbhub-server",
    "build:server": "tsc",
    "watch:ts": "tsc --watch",
    "watch:dev": "nodemon -r dotenv/config dist/index.js",
    "watch:full": "concurrently \"yarn watch:ts\" \"yarn watch:dev\"",
    "lint": "eslint ./src --ext .ts,.tsx"
  },
  "dependencies": {
    "compromise": "^13.10.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/pg": "^7.14.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "concurrently": "^6.0.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
}
