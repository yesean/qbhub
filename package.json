{
  "name": "qbc-server",
  "version": "1.0.0",
  "repository": "git@github.com:seanye24/qbc-server.git",
  "author": "Sean Ye <seanye24@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/index.js",
    "clean": "rm -rf dist",
    "build:ui": "cd ../qbhub-client && yarn build && cp -r build ../qbhub-server/dist && cd ../qbhub-server",
    "build:server": "tsc",
    "build": "yarn clean && yarn build:server && yarn build:ui",
    "deploy": "scp -r dist qbhub:~/qbhub-server/ && ssh qbhub 'pm2 restart qbhub'",
    "deploy:full": "yarn build && yarn deploy",
    "lint": "eslint ./src --ext .ts,.tsx",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec ts-node -r dotenv/config src/index.ts"
  },
  "dependencies": {
    "compromise": "^13.11.3",
    "compromise-ngrams": "^0.0.6",
    "compromise-sentences": "^0.3.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/pg": "^7.14.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "concurrently": "^6.0.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.2.4"
  }
}
