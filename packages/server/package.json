{
  "name": "@qbhub/server",
  "version": "1.0.0",
  "author": "Sean Ye <seanye24@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "start": "pm2 start pm2.config.js --no-daemon",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "compile": "tsc --build",
    "build": "yarn lint && yarn compile",
    "clean": "rm -rf dist",
    "lint": "tsc --build && tsc --noEmit && eslint --ext .ts,.tsx src",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@qbhub/types": "workspace:^",
    "compromise": "14.5.0",
    "compromise-stats": "0.1.0",
    "cors": "2.8.5",
    "express": "4.18.1",
    "pg": "8.8.0",
    "pino-http": "8.2.1",
    "pino-logflare": "0.3.12",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "8.24.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
