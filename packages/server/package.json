{
  "name": "@qbhub/server",
  "author": "Sean Ye <seanye24@gmail.com>",
  "description": "QBHub Node.js API",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@qbhub/types": "workspace:^",
    "@qbhub/utils": "workspace:^",
    "compromise": "14.5.0",
    "compromise-stats": "0.1.0",
    "cors": "2.8.5",
    "express": "4.18.1",
    "pg": "8.8.0",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "nodemon": "2.0.20",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "scripts": {
    "start": "pm2 start pm2.config.js --no-daemon",
    "dev": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:debug": "DEBUG=eslint:cli-engine yarn eslint",
    "lint": "yarn typecheck && yarn eslint",
    "lint:debug": "yarn typecheck && yarn eslint:debug",
    "format": "prettier src --write",
    "build": "tsc --build",
    "clean": "rm -rf dist dist-client tsconfig.tsbuildinfo"
  }
}
